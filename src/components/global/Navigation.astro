---
import links from '@/data/navigationLinks.json';

//links to services
const solutionsofferinglarge = links.solutionslinks.filter(
  (solution) =>
    solution.resolutionsize === 'large' && solution.type === 'offering'
);
const solutionsolutionslarge = links.solutionslinks.filter(
  (solution) =>
    solution.resolutionsize === 'large' && solution.type === 'solution'
);
const solutiontechlarge = links.solutionslinks.filter(
  (solution) =>
    solution.resolutionsize === 'large' && solution.type === 'technology'
);
const solutionsofferingsmall = links.solutionslinks.filter(
  (solution) =>
    solution.resolutionsize === 'small' && solution.type === 'offering'
);
const solutionsolutionssmall = links.solutionslinks.filter(
  (solution) =>
    solution.resolutionsize === 'small' && solution.type === 'solution'
);
const solutiontechsmall = links.solutionslinks.filter(
  (solution) =>
    solution.resolutionsize === 'small' && solution.type === 'technology'
);

//links to services
const servicesLeftlarge = links.servicesLinks.filter(
  (service) => service.resolutionsize === 'large' && service.column === 'left'
);
const servicesRightlarge = links.servicesLinks.filter(
  (service) => service.resolutionsize === 'large' && service.column === 'right'
);
const allservices = links.servicesLinks.filter(
  (service) => service.resolutionsize === 'small'
);

//links to industries
const industriesLeftlarge = links.industriesLinks.filter(
  (industry) =>
    industry.resolutionsize === 'large' && industry.column === 'left'
);
const industriessRightlarge = links.industriesLinks.filter(
  (industry) =>
    industry.resolutionsize === 'large' && industry.column === 'right'
);
const allindustries = links.industriesLinks.filter(
  (industry) => industry.resolutionsize === 'small'
);

//links to about
const allaboutlarge = links.allaboutLinks.filter(
  (about) => about.resolutionsize === 'large'
);
const allaboutsmall = links.allaboutLinks.filter(
  (about) => about.resolutionsize === 'small'
);

//links to about
const techcommunitieslarge = links.techcommunityLinks.filter(
  (techcommunity) => techcommunity.resolutionsize === 'large'
);
const techcommunitiessmall = links.techcommunityLinks.filter(
  (techcommunity) => techcommunity.resolutionsize === 'small'
);
---

<!-- Header -->
<header
  x-data='{ mobileNavOpen: false }'
  id='page-header'
  class='relative flex flex-none bg-neutral-800 dark:bg-stone-950 items-center py-6'
>
  <nav
    class='fixed w-full z-20 top-0 start-0 border-b-4 border-sky-800 bg-neutral-800 dark:bg-stone-950 items-center pb-6'
  >
    <!-- Main Header Content -->
    <div
      class='dark text-muted text-sm bg-[#002031] dark:bg-transparent dark:border-b dark:border-slate-800 dark:text-slate-100 hidden md:flex gap-1 overflow-hidden px-3 py-2 relative text-ellipsis whitespace-nowrap'
    >
      <div
        class='container relative mx-auto flex items-center justify-between px-4 lg:px-20 lg:max-w-7xl'
      >
        <a
          href='/blog/blog'
          class='text-muted px-5 hover:text-sky-700 dark:text-slate-400 text-slate-100 font-medium'
          >Blog</a
        >

        <a
          href='/'
          class='text-muted px-5 hover:text-sky-700 dark:text-slate-400 text-slate-100 font-medium'
          >Case Studies</a
        >

        <a
          href='/forms/contact'
          class='text-muted px-5 hover:text-sky-700 dark:text-slate-400 text-slate-100 font-medium'
